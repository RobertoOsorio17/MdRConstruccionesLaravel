APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=15
SESSION_ENCRYPT=true
SESSION_PATH=/
SESSION_DOMAIN=null
SESSION_SECURE_COOKIE=true
SESSION_HTTP_ONLY=true
SESSION_SAME_SITE=strict
SESSION_SERIALIZATION=json

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# OAuth Social Authentication
# Google OAuth - Get credentials from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Facebook OAuth - Get credentials from: https://developers.facebook.com/apps/
FACEBOOK_CLIENT_ID=
FACEBOOK_CLIENT_SECRET=

# GitHub OAuth - Get credentials from: https://github.com/settings/developers
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Google reCAPTCHA v3 - Get credentials from: https://www.google.com/recaptcha/admin
# Site Key: Used in frontend
# Secret Key: Used in backend validation
RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET_KEY=

# Google Maps API - Get API key from: https://console.cloud.google.com/google/maps-apis
# Enable Maps JavaScript API and Geocoding API
GOOGLE_MAPS_API_KEY=

# TinyMCE API Key - Get from: https://www.tiny.cloud/my-account/dashboard/
TINYMCE_API_KEY=

# Admin Panel Inactivity Detection
ADMIN_INACTIVITY_DETECTION_ENABLED=true
ADMIN_INACTIVITY_TIMEOUT=900000
ADMIN_INACTIVITY_WARNING_TIME=180000
ADMIN_HEARTBEAT_INTERVAL=120000
ADMIN_MAX_SESSION_LIFETIME=480
ADMIN_ALLOW_CONCURRENT_SESSIONS=false
ADMIN_LOG_INACTIVITY_EVENTS=true
ADMIN_NOTIFY_ON_INACTIVITY_LOGOUT=false
ADMIN_PLAY_WARNING_SOUND=false
ADMIN_DEBUG_INACTIVITY_DETECTOR=false

# Admin Panel Security
ADMIN_LOGOUT_ON_IP_CHANGE=false
ADMIN_LOGOUT_ON_USER_AGENT_CHANGE=false
